{
    "builds": [{
        "src": "djangoCalculusApp/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
        }
    }, {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }
    }],
    "routes": [{
        "src": "/calculusModule/static/(.*)",
        "dest": "/calculusModule/static/$1",
        "headers": {
            "Cache-Control": "max-age=31536000"
        }
    },
    {
        "src": "/(.*)",
        "dest": "djangoCalculusApp/wsgi.py"
    }]
}